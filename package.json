{
  "author": {
    "email": "darcher@insurefigo.com",
    "name": "Dylan Archer"
  },
  "dependencies": {
    "@types/next": "^9.0.0",
    "dotenv": "^16.0.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@jest/globals": "^28.0.0",
    "@jest/types": "^28.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.16.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.3"
  },
  "engine": {
    "node": ">=18.15.0",
    "npm": "please-use-npm"
  },
  "keywords": [
    "outline",
    "hirearchy",
    "micro-frontend",
    "frontend",
    "boilerplate",
    "dart scss",
    "documentation",
    "framework free",
    "guidelines",
    "javascript",
    "markdown",
    "styleguide",
    "svg icons",
    "template",
    "typescript",
    "vanilla css4",
    "vanilla jsx/tsx",
    "web components"
  ],
  "license": "UNLICENSED",
  "main": "dist/src/main.js",
  "name": "boilerplate-vts",
  "release": {
    "branches": [
      "main"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:darcher-figo/boilerplate.git"
  },
  "scripts": {
    "format": "node_modules/.bin/prettier --write --ignore-unknown '.'",
    "build": "node_modules/.bin/tsc --project 'tsconfig.build.json'",
    "dev": "node_modules/.bin/nodemon --ext '*' --exec && 'node --experimental-specifier-resolution=node --loader && ts-node/esm' 'src/main.ts'",
    "test": "node_modules/.bin/jest --colors --coverage '**/*.{ts,tsx}'",
    "lint": "node_modules/.bin/eslint --fix '**/*.{ts,tsx}'",
    "logs": "npx --no-install commitlint --config 'commitlint.config.cjs' --edit",
    "start": "node 'dist/src/main.js'",
    "clean": "rm -rf 'node_modules' && rm -rf 'dist' && rm -rf 'coverage' && rm -rf 'package-lock.json'"
  },
  "type": "module",
  "types": "dist/src/main.d.ts"
}
